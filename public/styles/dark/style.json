      "layout": { "visibility": "visible" },
      "paint": {
        "fill-color": "#163838",
        "fill-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          5,
          0.6,
          9,
          0.5,
          12,
          0.1
        ]
      }
    },
    {
